<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cille_000.laesomondo">

    <uses-sdk android:minSdkVersion="21" android:targetSdkVersion="21"/>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.cille_000.laesomondo.startscreen.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".startscreen.CreateUserActivity"></activity>
        <activity android:name=".mainscreen.MainActivity"></activity>

        <activity
            android:name=".challengescreen.ShowTextActivity"/>

    </application>

</manifest>